package Solution4;

import java.util.HashMap;
import java.util.Map;

class Solution4 {

	public int solution(String s) {

		int answer = 0;

		String[] alphabet = new String[s.length()];

		for (int i = 0; i < s.length(); i++) {
			alphabet[i] = String.valueOf(s.charAt(i));
		}

		// Map은 키값와 밸류값으로 정의된 컬렉션이다.
		Map<String, Integer> hm = new HashMap<>();

		for (String key : alphabet)

			// map의 getOrDefault내장함수는 파라미터 2개 키와 벨류값을 put으로 담는거다.
			// 먼저 hm.getOrDefault(키,밸류) key가 hm에 있니? 없으면 0 을 맨앞 파라미터 key에 담아준다.
			// 하지만 밸류뒤에 +1이 있기때문에
			// s:1이 들어갈것이다. 그리고 다음번에 또 s가 있니 물어보면 밸류값이 1이올꺼고 거기다 +1을 추가해서 2가 담길것이다.
			hm.put(key, hm.getOrDefault(key, 0) + 1);
		// (키값, 밸류값)

		for (int i = 0; i < alphabet.length; i++) {

			if (hm.containsValue(1)) {

			}
		}

		return answer;
	}

	public static void main(String[] args) {
		Solution4 T = new Solution4();
		System.out.println(T.solution("statitsics"));
		System.out.println(T.solution("aabb"));
		System.out.println(T.solution("stringshowtime"));
		System.out.println(T.solution("abcdeabcdfg"));
	}
}