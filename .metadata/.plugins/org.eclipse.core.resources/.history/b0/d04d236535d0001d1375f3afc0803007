package Solution;

import java.util.Arrays;

class Solution {

	public char[] solution(int n, int[][] ladder) {

		char[] answer = new char[n];

		// change number to Alphabet
		for (int i = 0; i < n; i++) {
			answer[i] = (char) (i + 65);
		}

		// looping Array 3times
		for (int[] line : ladder) {

			// looping Array 2times / first -> 1,3
			for (int x : line) {

				// first inside tmp 1
				char tmp = answer[x];

				answer[x] = answer[x - 1];
				answer[x - 1] = tmp;
			}
		}
		return answer;
	}

	public static void main(String[] args) {

		Solution T = new Solution();
		System.out.println(Arrays.toString(T.solution(5, new int[][] { { 1, 3 }, { 2, 4 }, { 1, 4 } })));

		System.out.println(Arrays.toString(T.solution(7, new int[][] { { 1, 3, 5 }, { 1, 3, 6 }, { 2, 4 } })));

		System.out.println(
				Arrays.toString(T.solution(8, new int[][] { { 1, 5 }, { 2, 4, 7 }, { 1, 5, 7 }, { 2, 5, 7 } })));

		System.out.println(Arrays.toString(T.solution(12,
				new int[][] { { 1, 5, 8, 10 }, { 2, 4, 7 }, { 1, 5, 7, 9, 11 }, { 2, 5, 7, 10 }, { 3, 6, 8, 11 } })));
	}

}